<form action="/bookmarks" method="POST" id="new_bookmark">
	<input type="text" size="30" name="url" value="Paste URL Here" />
	<input type="submit" value="Go!" />
</form>

<div class="bookmarks">
	<ul>
	<% @bookmarks["bookmarks"].each do |bookmark| %>
		<li><%= bookmark["url"]%></li>
	<% end %>
	</ul>
	
</div>
